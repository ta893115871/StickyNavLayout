<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gxz.stickynavlayout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.SimpleStickActivity"
            android:label="SimpleStickActivity" />
        <activity
            android:name=".activity.PullToRefreshStickActivity"
            android:label="PullToRefreshStickActivity" />
        <activity android:name=".activity.TopViewOverOneScreenActivity"></activity>
    </application>

</manifest>
